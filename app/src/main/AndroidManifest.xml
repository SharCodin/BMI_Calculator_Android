            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3534032287949019~5851281925" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

